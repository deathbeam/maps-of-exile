{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": "https://mapsofexile.com",
  "scripts": {
    "fix": "eslint --fix src --ext js,jsx && prettier --write '{*,public/*,src/**,src/**/*,src/**/**/*}.{js,jsx,css,scss,md,html}'",
    "start": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@preact/signals": "^1.2.1",
    "bootstrap": "5.3.2",
    "lodash.debounce": "^4.0.8",
    "preact": "^10.16.0",
    "react-router-dom": "^6.18.0",
    "react-select-search": "^4.1.7",
    "reactflow": "^11.9.4"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "browserslist-to-esbuild": "^1.2.0",
    "eslint": "^8.53.0",
    "eslint-config-preact": "^1.3.0",
    "prettier": "^3.0.3",
    "vite": "^4.4.5"
  },
  "eslintConfig": {
    "extends": "preact",
    "settings": {
      "jest": {
        "version": "latest"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
